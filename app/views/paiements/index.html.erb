<p id="notice"><%= notice %></p>

<h1>Paiements</h1>

<table>
  <thead>
    <tr>
      <th>Roof</th>
      <th>Plafond</th>
      <th>Mode</th>
      <th>Adress 1</th>
      <th>Adress 2</th>
      <th>Cp</th>
      <th>City</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @paiements.each do |paiement| %>
      <tr>
        <td><%= paiement.roof %></td>
        <td><%= paiement.plafond %></td>
        <td><%= paiement.mode %></td>
        <td><%= paiement.adress_1 %></td>
        <td><%= paiement.adress_2 %></td>
        <td><%= paiement.cp %></td>
        <td><%= paiement.city %></td>
        <td><%= paiement.country %></td>
        <td><%= link_to 'Show', paiement %></td>
        <td><%= link_to 'Edit', edit_paiement_path(paiement) %></td>
        <td><%= link_to 'Destroy', paiement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paiement', new_paiement_path %>
